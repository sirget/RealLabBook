@model IEnumerable<RealLabBook.Models.Blacklist>

@{
    ViewData["Title"] = "Submit";
}

<h1>Submited</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                BookingID
            </th>
            <th>
                ToolID
            </th>
            <th>
                UserID
            </th>
            <th>
                Start_time
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in ViewData["Bookedlist"] as List<Booking>)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.BookingID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ToolID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.start_time)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.BookingID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.BookingID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.BookingID">Delete</a>
            </td>
        </tr>
        }
    </tbody>
</table>
